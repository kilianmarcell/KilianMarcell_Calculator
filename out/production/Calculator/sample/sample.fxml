      <Button alignment="CENTER" contentDisplay="CENTER" maxHeight="30" maxWidth="40" minHeight="30" minWidth="40" mnemonicParsing="false" text="/" textAlignment="CENTER" GridPane.columnIndex="3" GridPane.rowIndex="1" />
      <Button alignment="CENTER" contentDisplay="CENTER" maxHeight="30" maxWidth="40" minHeight="30" minWidth="40" mnemonicParsing="false" text="/" textAlignment="CENTER" GridPane.columnIndex="4" GridPane.rowIndex="1" />
   </children>
</GridPane>
